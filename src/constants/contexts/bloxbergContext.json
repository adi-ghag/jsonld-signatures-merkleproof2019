{
  "@context": {
    "id": "@id",
    "type": "@type",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "sec": "https://w3id.org/security#",
    "schema": "https://schema.org/",
    "bloxberg": "https://bloxberg.org/terms#",

    "VerifiableCredential": "https://www.w3.org/2018/credentials#VerifiableCredential",
    "BloxbergCredential": "bloxberg:BloxbergCredential",

    "issuer": {"@id": "https://www.w3.org/2018/credentials#issuer", "@type": "@id"},
    "issuanceDate": {"@id": "https://www.w3.org/2018/credentials#issuanceDate", "@type": "xsd:dateTime"},
    "credentialSubject": {"@id": "https://www.w3.org/2018/credentials#credentialSubject", "@type": "@id"},

    "issuingOrg": {"@id": "bloxberg:issuingOrg", "@type": "@id"},
    "displayHtml": {"@id": "bloxberg:displayHtml", "@type": "xsd:string"},
    "crid": {"@id": "bloxberg:crid", "@type": "xsd:string"},
    "cridType": {"@id": "bloxberg:cridType", "@type": "xsd:string"},
    "metadataJson": {"@id": "bloxberg:metadataJson", "@type": "xsd:string"},

    "proof": {"@id": "sec:proof", "@type": "@id", "@container": "@graph"},
    "MerkleProof2019": "sec:MerkleProof2019",
    "created": {"@id": "https://purl.org/dc/terms/created", "@type": "xsd:dateTime"},
    "proofValue": {"@id": "sec:proofValue", "@type": "sec:multibase"},
    "proofPurpose": {"@id": "sec:proofPurpose", "@type": "@vocab"},
    "verificationMethod": {"@id": "sec:verificationMethod", "@type": "@id"},
    "assertionMethod": {"@id": "sec:assertionMethod", "@type": "@id"},
    "ens_name": {"@id": "bloxberg:ensName", "@type": "xsd:string"}
  }
}